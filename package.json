  "dependencies": {
    "@types/nedb": "^1.8.11",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multiview": "^3.0.1",
    "nedb": "^1.8.0",
    "supervisor": "^0.12.0",
    "typescript": "^4.3.2",
    "yarn": "^1.22.10"
